.tablebox_02 {
	border: 1px solid #cfd7d5;
	text-align: center;
	background-color:#c0c0c0;
	width: auto;
	height: auto;
}
.tablebox_02 td {
   	word-wrap:break-word; 
	word-break:break-all;
	white-space:normal;
	border-right-width: 1px;
	height: 21px;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #cfd7d5;
	border-right-width: 1px;
	border-right-style: solid;
	border-right-color: #cfd7d5;
	padding-left: 2px;
	padding-right: 2px;
}
.tablebox_02 .tablehead {
    border: 1px solid #aaa;
    margin: 0px;
    padding: 0;
    height: 25px;
    line-height: 18px;
    color: #333;
    background-color: #fafafa;
    text-align: center;
    font-weight: bold;
    word-break: keep-all;
    white-space: nowrap;
}
.tablebox_02 .tablehead td{
	padding: 6px 4px;
}
.tablebox_02 .tablelist {
	overflow: auto;
	width: auto;
}
.tablebox_02 table,
.tablebox_02 .tablelist table{
	border-collapse: collapse;
    border-spacing: 0px;
    table-layout: fixed;
    width: 100%;
}
.tablebox_02 .tablelist td {
	height: 25px;
	padding: 6px 4px;
	text-align: left;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    border: 1px solid #ccc;
    color: #333;
}
.tablebox_02  .tablelist_now {
	background-color: #fffcc9;
}
.tablebox_02 .ico_now {
	width: 15px;
	background-image: url(../images/ico_01.jpg);
	background-repeat: no-repeat;
	background-position: center center;
}

#--------------------------下拉Grid专用样式-----------------------------
.comboDiv{
	height: 20px;
	z-index:3;	
	margin-right: 20px;
}

.comboDiv .textDiv{
	width:100%;
	height:100%;	
	border:1px solid #AAAAAA;
}
.comboDiv .loadDiv{
	height:20px;	
	background:url("../images/loading.gif") no-repeat left;
	/*background-color:green;*/
}
.gridDiv{
	position:absolute;
	background-color: white;
	overflow: auto;
	z-index: 90999;
}
.textDiv input{
	height: 30px;
    line-height: 16px;
    padding: 8px 10px;
    border: 1px solid #cdd2d4;
    float: left;
    box-sizing: border-box;
    box-shadow: 0 2px 3px rgba(0, 0, 0, 0.1) inset;
}
.textDiv p{
	clear: both;
}
.textDiv a.combobox-btn{
	width:22px;
	height:24px;
	display:block;
	float: left;
	color: #777;
    font-size: 18px;
    border: 1px solid #cfd7d5;
    padding: 4px 0 0 4px;
}
.textDiv a.combobox-btn:LINK{
	
}
.textDiv a.combobox-btn:HOVER{
	color: #333;
}
.textDiv a.combobox-btn:ACTIVE{
